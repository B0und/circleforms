{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:dd.MM.yyyy HH:mm:ss.fff}] [{Level}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/data.log",
          "outputTemplate": "[{Timestamp:dd.MM.yyyy HH:mm:ss.fff}] [{Level}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "PathBase": "/",
  
  "osuApi": {
    "ClientId": 0,
    "ClientSecret": "",
    "CallbackUrl": "" 
  },
  "ConnectionStrings": {
    "Database": "",
    "Redis": ""
  },
  "SuperAdminsId": "100,101"
}
