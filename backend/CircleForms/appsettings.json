{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:dd.MM.yyyy HH:mm:ss.fff}] [{Level}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "./logs/main.log",
          "outputTemplate": "[{Timestamp:dd.MM.yyyy HH:mm:ss.fff}] [{Level}] {Message:lj}{NewLine}{Exception}",
          "rollingInterval": "Month"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "PathBase": "/",
  "StaticFiles": {
    "VolumePath": "/db"
  },
  "osuApi": {
    "ClientId": 0,
    "ClientSecret": "",
    "CallbackUrl": ""
  },
  "ConnectionStrings": {
    "Database": "",
    "Redis": ""
  },
  "SuperAdmins": {
    "Ids": [
      100,
      101
    ]
  }
}
